package burracoEquo;

import java.util.ArrayList;

public class Pozzetto {
	
	private ArrayList<Carta> pozzo;
	
	

	public Pozzetto(ArrayList<Carta> pozzo) {
		super();
		this.pozzo = pozzo;
	}

	public ArrayList<Carta> getPozzo() {
		return pozzo;
	}

	public void setPozzo(ArrayList<Carta> pozzo) {
		this.pozzo = pozzo;
	}

}
